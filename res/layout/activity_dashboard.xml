<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:layout_gravity="right"
            android:orientation="horizontal" >

        <ImageButton
                android:id="@+id/dashboard_quitExperiment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="onClick_openAppSettings"
                android:src="@drawable/icon_settings"
                android:background="@null"/>

    </LinearLayout>

    <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:orientation="horizontal" >

            <TextView
            android:id="@+id/dashboard_textExperimentRunning"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_weight="2"
            android:text="@string/dashboard_text_experiment_running"
            android:textAllCaps="true"
            android:textAppearance="@style/ui_font_title" />
<!-- 
        <Button
            android:id="@+id/dashboard_quitExperiment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:onClick="onClick_quitExperiment"
            android:text="@string/dashboard_quit_experiment" />
 -->

    </LinearLayout>




    <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_gravity="center_horizontal">



        <RelativeLayout
                android:id="@+id/dashboard_ExperimentTimeElapsed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                >

        <ImageView
                android:id="@+id/dashboard_ExperimentTimeElapsed1"
                android:layout_width="100dp"
                android:layout_height="10dp"
                android:src="@drawable/dark_blue_rect_button"
                android:layout_marginBottom="10dp"

                />

        <TextView
                android:id="@+id/dashboard_ExperimentTimeElapsed2"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_below="@id/dashboard_ExperimentTimeElapsed1"
                android:textColor="@color/ui_dark_blue_color"
                android:text="XX"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:gravity="center"
                android:background="@drawable/white_rectangle"

                />

            <TextView
                    android:id="@+id/dashboard_ExperimentTimeElapsed2days"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/dashboard_ExperimentTimeElapsed2"
                    android:textColor="@color/ui_dark_blue_color"
                    android:text="days"
                    android:gravity="center"
                    android:background="@drawable/white_rectangle"/>



        </RelativeLayout>



        <RelativeLayout
                android:id="@+id/dashboard_ExperimentSeeResults"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:layout_toRightOf="@id/dashboard_ExperimentTimeElapsed"
                >

        <ImageView
                android:id="@+id/dashboard_ExperimentResultsIn1"
                android:layout_width="100dp"
                android:layout_height="10dp"
                android:src="@drawable/white_rectangle"
                android:layout_marginBottom="10dp"
                />


            <!--
        <ImageView
                android:id="@+id/dashboard_ExperimentArrow"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:layout_below="@id/dashboard_ExperimentResultsIn1"
                android:src="@drawable/black_background"
                />
                -->


        <TextView
                android:id="@+id/dashboard_ExperimentResultsIn2"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_below="@id/dashboard_ExperimentResultsIn1"
                android:textColor="@color/white"
                android:text="YY"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:gravity="center"
                android:background="@drawable/dark_blue_rectangle"
                android:src="@drawable/settings_icon"
                />

            <TextView
                    android:id="@+id/dashboard_ExperimentResultsIn2days"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/dashboard_ExperimentResultsIn2"
                    android:textColor="@color/white"
                    android:text="days"
                    android:gravity="center"
                    android:background="@drawable/dark_blue_rectangle"
                    android:src="@drawable/settings_icon"
                    />

        <TextView
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_below="@id/dashboard_ExperimentResultsIn2days"
                android:layout_toRightOf="@id/dashboard_ExperimentTimeElapsed1"
                android:textAppearance="@style/ui_font_button_dark"
                android:text="See Results"
                android:gravity="center"
                android:background="@drawable/white_rectangle"
                android:layout_marginTop="10dp"
                android:src="@drawable/settings_icon"
                />

        </RelativeLayout>

        <!--
                <Button
                    android:id="@+id/dashboard_quitExperiment"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:onClick="onClick_quitExperiment"
                    android:text="@string/dashboard_quit_experiment" />



         -->
    </RelativeLayout>


    <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_below="@id/dashboard_ExperimentSeeResults"
                android:orientation="horizontal" >


            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/settings_button_terms"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="10dp"
                    />


            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/settings_button_description"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="10dp"
                    />

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/settings_button_about"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="10dp"
                    />


        </LinearLayout>








